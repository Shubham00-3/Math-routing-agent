    "steps": [
      {
        "step_number": 1,
        "description": "Apply the power rule to each term",
        "explanation": "The power rule states d/dx[xⁿ] = n·xⁿ⁻¹",
        "formula": "d/dx[xⁿ] = n·xⁿ⁻¹"
      },
      {
        "step_number": 2,
        "description": "Differentiate 3x³",
        "explanation": "3 × 3 × x³⁻¹ = 9x²",
        "formula": "d/dx[3x³] = 9x²"
      },
      {
        "step_number": 3,
        "description": "Differentiate 2x²",
        "explanation": "2 × 2 × x²⁻¹ = 4x",
        "formula": "d/dx[2x²] = 4x"
      },
      {
        "step_number": 4,
        "description": "Differentiate -x",
        "explanation": "The derivative of x is 1",
        "formula": "d/dx[-x] = -1"
      },
      {
        "step_number": 5,
        "description": "Differentiate the constant 5",
        "explanation": "The derivative of any constant is 0",
        "formula": "d/dx[5] = 0"
      },
      {
        "step_number": 6,
        "description": "Combine all terms",
        "explanation": "Add all the derivatives together",
        "formula": "f'(x) = 9x² + 4x - 1 + 0"
      }
    ],
    "final_answer": "f'(x) = 9x² + 4x - 1",
    "confidence_score": 0.98
